{
	"singleQuote": true,
	"arrowParens": "always",
	"max-len": ["error", 120, 2],
	"tabWidth": 2,
	"semi": false,
	"useTabs": true,
	"trailingComma": "none",
	"bracketSpacing": true,
	"bracketSameLine": false,
	"importOrder": [
		"<THIRD_PARTY_MODULES>",
		"^[./]"
	],
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true
}
